{
  "compilerOptions": {
    "target": "ESNext",
    "allowJs": true,
    "allowNonTsExtensions": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "reactNamespace": "React",
    "module": "ESNext",
    "moduleResolution": "Node",
    "sourceMap": true,
    "useDefineForClassFields": false,
    "paths": {
      "~/*": [
        "*"
      ],
      "@divriots/starter-sass/*": [
        "*"
      ]
    }
  }
}